<h4>User検索</h4>
<div class="search-form">
  <%= form_with url: usersearch_path, method: :get, local: true do |f| %>
    <%= f.text_field :key_word %>
    <%= f.hidden_field :name, :value => 'name' %>
    <%= f.submit "検索" %>
  <% end %>
</div>
<div class='col-md-8 offset-md-1'>
 <h2>Follow Users</h2>
 <%= render 'follow_list', customer: @customers %>
</div>